<%- partial('../partials/header')%>


<div class="container">
  <div class="page-header"><%= actionResult %></div>

    <form class="form-horizontal" action="destroy" method="post">
    <fieldset>

      <legend><%= i18n('Remove holder') %></legend>

      <div class="control-group">
        <label class="control-label" for="input_name"><%= i18n('Name') %></label>
        <div class="controls">
          <select>
            <% _.each(holders, function (h) { %>
              <option><%= h.name %></option>
            <% }) %>            
          </select>
        </div>
      </div>

      <div class="control-group">
        <div class="controls">
          <button id="input_submit" name="input_submit" class="btn btn-primary"><%= i18n('Remove holder') %></button>
        </div>
      </div>

    </fieldset>
  </form>
</div>



<%- partial('../partials/footer') %>